<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="in.hoptec.exploman.Home">




    <ImageView
          android:id="@+id/sss"
        android:scaleType="fitXY"
        android:src="@drawable/mapbg"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.daniribalbert.customfontlib.views.CustomFontTextView
        app:font="LearningCurve"
        android:textSize="25dp"
        android:visibility="gone"

        android:gravity="center"
        android:text="DEMO LIMITED \nCONTENT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sss"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="34dp"
        android:id="@+id/customFontTextView4" />




</RelativeLayout>
